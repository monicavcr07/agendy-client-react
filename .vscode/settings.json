{
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "files.associations": { "*.js": "javascriptreact" },
  "[javascriptreact]": {
    "editor.defaultFormatter": "remimarsal.prettier-now"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "prettier.arrayExpand": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.semi": false,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ffffff",
    "activityBar.activeBorder": "#df9f9f",
    "activityBar.background": "#ffffff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#df9f9f",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#ffffff",
    "statusBar.background": "#f6f6f6",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#dcdcdc",
    "statusBarItem.remoteBackground": "#f6f6f6",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#f6f6f6",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#f6f6f699",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "f6f6f6"
}
